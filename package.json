{
  "name": "tibia-website",
  "version": "1.0.0",
  "description": "A Tibia-themed website",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "build:js": "tsc && terser public/js/home.js -c -m -o public/js/home.min.js && del /Q public\\js\\home.js",
    "build:all": "pnpm run build && pnpm run build:js"
  },
  "keywords": [
    "tibia",
    "nodejs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=16.14.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.1",
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.1.1",
    "@fastify/view": "^10.0.2",
    "@prisma/client": "^6.4.0",
    "@types/connect-redis": "^0.0.23",
    "@types/ioredis": "^4.28.10",
    "@types/redis": "^4.0.10",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-safe": "^9.1.0",
    "ejs": "^3.1.10",
    "fastify": "^5.2.1",
    "fastify-cors": "^6.1.0",
    "fastify-helmet": "^7.1.0",
    "fastify-rate-limit": "^5.9.0",
    "fastify-redis": "^4.4.0",
    "fastify-session": "^5.2.1",
    "helmet": "^8.0.0",
    "helmet-csp": "^4.0.0",
    "ioredis": "^5.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "pg": "^8.13.3",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.4.0",
    "rate-limiter-flexible": "^5.0.5",
    "redis": "^4.7.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.13",
    "@types/ejs": "^3.1.5",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "fastify-plugin": "^5.0.1",
    "terser": "^5.39.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "type": "commonjs"
}
